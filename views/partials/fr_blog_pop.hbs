<article class="post hentry item-0">
    <a class="entry-thumbnail"
        href="/fr/articles/{{this.fr_slug}}"
        title="{{this.fr_title}}"><span class="thumbnail pbt-lazy"
            data-src="/images/{{this.fr_thumb}}" style="background-image:url('/images/{{this.fr_thumb}}')"></span></a>
    <div class="entry-header">
        <a href="/fr/articles/categorie/{{this.cat_slug}}"><span class="entry-tag">{{this.fr_name}}</span></a>
        <h2 class="entry-title"><a
                href="/fr/articles/{{this.fr_slug}}"
                rel="bookmark">{{this.fr_title}}</a></h2>
        <p class="entry-excerpt excerpt"></p>
        <div class="entry-meta">
            <span class="entry-author"><span class="author-name"></span></span>
            <span class="entry-time"><span class="sp"></span><time class="published"
                    datetime="2022-04-12T11:44:00-07:00"></time></span>
        </div>
    </div>
</article>